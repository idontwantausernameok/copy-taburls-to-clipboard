{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "copy urls"
  },
  "description": "copy tab urls of active window into the clipboard",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "tabs2clip",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "notifications"
  ],
  "version": "1.0.2"
}
