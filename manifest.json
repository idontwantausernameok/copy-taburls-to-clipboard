{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+O"
      }
    }
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "copy urls"
  },
  "description": "copy tab urls of active window into the clipboard",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "copy-tabs-urls-to-clipboard",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "notifications"
  ],
  "version": "1.0.5"
}
