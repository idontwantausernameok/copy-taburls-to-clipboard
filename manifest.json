{
	"manifest_version": 2,
	"name": "tab2clip",
	"description": "copy tab urls of active window into the clipboard",
	"version": "1.0.0",
	"author": "igorlogius",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "copy urls"
	},
	"permissions": [
		"tabs",
		"clipboardWrite",
		"notifications"
	]
}
